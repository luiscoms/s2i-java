{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "softtech-s2i-new-relic-template"
  },
  "labels": {
    "app": "s2i-new-relic",
    "template": "softtech-s2i-new-relic-template"
  },
  "objects": [
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "s2i-new-relic"
      }
    },
    {
      "apiVersion": "v1",
      "kind": "BuildConfig",
      "metadata": {
        "name": "s2i-new-relic"
      },
      "spec": {
        "runPolicy": "SerialLatestOnly",
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "source": {
          "git": {
            "uri": "https://github.com/softtechconsulting/s2i-new-relic"
          },
          "contextDir": "1.8-gradle",
          "sourceSecret": {
            "name": "github-token"
          },
          "type": "Git"
        },
        "strategy": {
          "dockerStrategy": {
            "noCache": true
          }
        },
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "s2i-new-relic:0.1.0"
          }
        }
      }
    }
  ]
}
